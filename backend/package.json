{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "pc": "echo 'Running backend pre-commit hooks...' && npm run update-version",
    "update-version": "echo 'updating server version...' && ts-node update-version.ts",
    "pc-message": "echo 'Running backend pre-commit hooks...'",
    "prod": "node dist/app.js",
    "build": "npx tsc",
    "start": "npx ts-node ./bin/www.ts"
  },
  "pre-commit": ["pc-message", "update-version"],
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.16.0",
    "@webgpu/types": "^0.1.40",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "jade": "^0.31.2",
    "morgan": "~1.9.1",
    "pre-commit": "^1.2.2",
    "spacy": "^0.0.4",
    "typescript": "^5.3.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/webgl-ext": "^0.0.37",
    "@types/ws": "^8.5.10"
  }
}
